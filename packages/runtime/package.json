{
  "name": "@escalier/runtime",
  "version": "0.0.1",
  "description": "Runtime library for the Escalier programming language",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly --declaration --outDir dist",
    "test": "vitest run"
  },
  "keywords": [
    "escalier",
    "runtime",
    "programming-language",
    "typescript"
  ],
  "author": "escalier-lang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/escalier-lang/escalier.git",
    "directory": "packages/runtime"
  },
  "homepage": "https://github.com/escalier-lang/escalier#readme",
  "bugs": {
    "url": "https://github.com/escalier-lang/escalier/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
